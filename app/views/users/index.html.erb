<% provide(:title, 'All users') %>
<h1>All users</h1>

<%= will_paginate %>

<ul class="users">
  <%# Rails infers that @users is a list of User objects automatically iterates %>
  <%# through them and renders each one with the _user.html.erb partial %>
  <%# (inferring the name of the partial from the name of the class). %>
  <%= render @users %>
</ul>

<%= will_paginate %>
